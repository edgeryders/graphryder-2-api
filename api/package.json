{
    "name": "graphryder-graphql-api",
    "version": "0.1",
    "description": "A backend for Graphryder 2.0.",
    "repository": {
      "type": "git",
      "url": "https://github.com/edgeryders/graphryder-2-api"
    },
    "scripts": {
      "start": "concurrently --kill-others \"webpack --watch\" \"nodemon build/bundle.js\"",
      "test": "mocha",
      "build": "better-npm-run build",
      "serve": "better-npm-run serve"
    },
    "betterScripts": {
      "build": {
        "command": "webpack",
        "env": {
          "NODE_ENV": "production"
        }
      },
      "serve": {
        "command": "node build/bundle.js",
        "env": {
          "NODE_ENV": "production"
        }
      }
    },
    "dependencies": {
      "@sendgrid/mail": "^6.3.1",
      "apollo-server": "2.17.0",
      "apollo-server-express": "2.17.0",
      "axios": "^0.18.0",
      "better-npm-run": "^0.1.0",
      "body-parser": "^1.18.2",
      "concurrently": "^3.5.1",
      "cors": "^2.8.4",
      "dotenv": "^4.0.0",
      "eslint": "^4.13.0",
      "eslint-config-airbnb-base": "^12.1.0",
      "eslint-loader": "^1.9.0",
      "eslint-plugin-import": "^2.8.0",
      "express": "^4.16.2",
      "graphql": "15.3.0",
      "graphql-resolvers": "0.4.2",
      "graphql-tools": "6.2.0",
      "keycloak-connect": "^11.0.2",
      "keycloak-connect-graphql": "^0.6.3",
      "lodash": "^4.17.11",
      "mime": "^2.0.3",
      "mocha": "^4.0.1",
      "neo4j-driver": "4.1.1",
      "node-cron": "^2.0.3",
      "nodemon": "^1.12.4",
      "normalize-url": "^4.0.0",
      "path": "^0.12.7",
      "plato-core": "github:Edgeryders-Participio/plato-core#keycloak-migration",
      "pug": "^2.0.3",
      "source-map-support": "^0.5.0",
      "subscriptions-transport-ws": "^0.9.15",
      "uuid": "^3.3.2",
      "webpack": "^3.10.0",
      "webpack-node-externals": "^1.6.0"
    },
    "devDependencies": {
      "babel-cli": "^6.26.0",
      "babel-preset-es2015": "^6.24.1"
    }
  }